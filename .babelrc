{
  "presets": [
    "@babel/env",
    "@babel/flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining"
  ],


  "env": {
    "commonjs": {
      "presets": [
        ["@babel/env", {
          "useBuiltIns": "usage"
        }]
      ]
    },
    "es": {
      "presets": [
        ["@babel/env", {
          "useBuiltIns": "usage",
          "modules": false
        }]
      ]
    }
  }
}
